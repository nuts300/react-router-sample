{
    "env": {
        "node": true,
        "browser": true,
        "es6": true,
        "commonjs": true
    },
    "plugins": [
        "react",
        "flowtype"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "flowtype/define-flow-type": 1,
        "flowtype/use-flow-type": 1
    }
}